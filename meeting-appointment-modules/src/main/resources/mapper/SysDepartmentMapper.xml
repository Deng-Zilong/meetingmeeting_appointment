<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jfzt.meeting.mapper.SysDepartmentMapper">

    <resultMap id="BaseResultMap" type="com.jfzt.meeting.entity.SysDepartment">
        <id property="id" column="id" jdbcType="BIGINT"/>
        <result property="departmentId" column="department_id" jdbcType="BIGINT"/>
        <result property="departmentName" column="department_name" jdbcType="VARCHAR"/>
        <result property="parentId" column="parent_id" jdbcType="BIGINT"/>
        <result property="isDeleted" column="is_deleted" jdbcType="TINYINT"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,department_id,department_name,
        parent_id,is_deleted
    </sql>
    <insert id="insertAll" parameterType="com.jfzt.meeting.entity.SysDepartment">
        insert into customer (department_id,department_name,parent_id)
        values
    <foreach collection="sysDepartmentLists" item="id" index="index" separator=",">
        (#{department_id,jdbcType=BIGINT}, #{departmentName,jdbcType=VARBINARY}, #{parentId,jdbcType=BIGINT})
    </foreach>

    </insert>
</mapper>
